package com.java.first;

import java.util.Arrays;

public class AlphabeticOrder {

	public static void main(String[] args) {
		String name = "Rohit Singh";
		
		
		if(name.contains(" ")) {
			name.replace(" ","");
		}
		
		char[] ch = name.toCharArray();
		Arrays.sort(ch);
		System.out.println(ch);
		
		
	}

}
